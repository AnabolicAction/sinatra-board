<div class="container">
    <div class="text-center my-3">
        <h1>게시판이다 </h1>
          
      <div class="text-right">
        <a class="btn btn-success" href="/posts/new">글싸지르기</a>
      </div>
     
   <table class="table">
      <thead>
        <tr>
          <th scope="col">이름</th>
          <th scope="col">이메일</th>
          <th scope="col">비밀번호</th>
        </tr>
      </thead>
      <tbody>
          <% @users.reverse.each do|p| %>
        <tr>
          <td><%= p.name%></td>
          <td><%= p.email%></td>
          <td><%= p.pwd%></td>
          <td><a href="users/<%= p.id%>"> 링크</a></td>
        </tr>
      <% end%>
      </tbody>
</table>
</div>